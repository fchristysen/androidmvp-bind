<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="org.greenfroyo.androidmvp_bind">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".app.MVPBApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".app.home.HomeActivity">
        </activity>
        <activity android:name=".app.intentparam.front.IntentParamFrontActivity">
        </activity>
        <activity android:name=".app.intentparam.back.IntentParamBackActivity">
        </activity>
        <activity android:name=".app.twoway.TwoWayActivity">
        </activity>
        <activity android:name=".app.compoundview.CompoundViewActivity">
        </activity>
        <activity android:name=".app.login.LoginActivity">
        </activity>
        <activity android:name=".app.multitab.MultiTabActivity"/>
        <activity android:name=".app.chat.ChatActivity"/>
        <activity
            android:name=".app._core.material.ScrollingActivity"
            android:label="@string/title_activity_scrolling"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>